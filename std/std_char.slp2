uses rstd;
procedure PutChar(c: char);
begin
    rstd::rc_putchar(c);
end;

procedure PutInt32(c: int32);
begin
    rstd::rc_putint32(c)
end;

procedure PutInt64(c: int64);
begin
    rstd::rc_putint64(c)
end;

procedure PutString(var c: string);
begin
    rstd::rc_putstring(c)
end;

function GetChar():char;
begin
    Result := rstd::rc_getchar();
end;
