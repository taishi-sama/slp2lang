uses std_char;

type
    arr = array [1..10] of int32;

procedure update(var a: arr);
begin
    var i := 1; 
    while i <= 10 do
    begin
        a[i] := i;
        i := i + 1
    end;
end;

begin
    var a : arr; //:= new array [1..10] of int32(0);
    update(a);
    var i := 1;
    while i <= 10 do
    begin
        std_char::PutChar( (int32('Ğ°') + a[i] - 1) as char );
        i := i + 1
    end;
    std_char::PutChar('\n');
    Result := 0;
end.