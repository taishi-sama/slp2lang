uses std_char;

type
    arr = array [1..10] of int32;

procedure update(var a: arr) 
begin
    var i := 1; 
    while i <= 10 do
    begin
        a[i] := i;
        i := i + 1
    end;
end;

begin
    var a : arr;
    update(a);
    var i := 1;
    while i <= 10 do
    begin
        std_char::putchar( (int32('a') + a[i]) as char );
        i := i + 1
    end;
    Result := 0;
end.